@model ParentsPortal.Models.StudentCourse

@{
    ViewData["Title"] = "Details";
}

    <h2>StudentCourse Details </h2>

<div>
    <h4>StudentCourse</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.StudentInfo)
        </dt>
        <dd>
            <span>Student Id :</span>  @Html.DisplayFor(model => model.StudentInfo.StudentId)
        </dd>
        <dd>
            <span>Student Name :</span>  @Html.DisplayFor(model => model.StudentInfo.StudentName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Course)
