        <dd>
            <span>Course Id :</span>  @Html.DisplayFor(model => model.Course.CourseId)
        </dd>
        <dd>
            <span>Course Name :</span>  @Html.DisplayFor(model => model.Course.CourseName)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Semester)
        </dt>
        <dd>
         <span>Semester :</span>   @Html.DisplayFor(model => model.Semester)
        </dd>
